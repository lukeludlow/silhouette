    "modmenu": [
      "dev.lukel.silhouette.options.ui.SilhouetteModMenu"
    ]
  },
  "mixins": [
    "silhouette.mixins.json"
  ],
  "accessWidener": "silhouette.accesswidener",

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
